                # Se tiver tarefa fora da caixa
                if verificar_processos_fora_das_caixas():
                    caixas[0][cont].click()
                    # AQUI VERIFICA OS PROCESSOS FORA DAS SUBCAIXAS

            # Verificar a posição do "(" que é a primeira letra após o nome da caixa
            posicao = caixas[0][cont].text.find("(") - 1
            # Adicionar o nome da caixa à lista de caixas já verificadas
            nomes_caixas.append(caixas[0][cont].text[:posicao])
    else:
        # Se o contador passar da quantidade de caixas encerra o loop
        cont = 100
    cont = cont + 1

pagina.close()

print("Teste")
