#Importando Módulos e Bibliotecas
from selenium import webdriver
from selenium.webdriver.common.by import By
from selenium.webdriver.support import expected_conditions as EC
from selenium.webdriver.support.wait import WebDriverWait as WDW
from selenium.webdriver.common.action_chains import ActionChains as AC
from time import sleep

def atualizar_caixas():
    # Refrescando as variáveis para não ter problema de elemento perdido pelo tempo sem usar
    pagina.refresh()
    # Localizar caixas de trabalho e as setas
    caixas = WDW(pagina, 60).until(EC.presence_of_all_elements_located((By.CLASS_NAME, "rich-tree-node-text")))
    # Identificar setas que não foram clicadas
    setas_collapsed = pagina.find_elements(By.CLASS_NAME, "rich-tree-node-handleicon-collapsed")
    # Identificar setas que foram clicadas
    setas_expanded = pagina.find_elements(By.CLASS_NAME, "rich-tree-node-handleicon-expanded")
    return caixas, setas_collapsed, setas_expanded

def verificar_processos_fora_das_caixas():
    posicao1 = caixas[0][cont].text.find("(") + 1
    posicao2 = caixas[0][cont].text.find(")")
    qtd_tarefas = int(caixas[0][cont].text[posicao1:posicao2])
    posicao3 = caixas[0][cont].text.find("-") + 1
    novo_texto = caixas[0][cont].text[posicao3:]
    print(novo_texto)
    posicao4 = novo_texto.find("(") + 1
    posicao5 = novo_texto.find(")")
    qtd_tarefas_em_caixas = int(novo_texto[posicao4:posicao5])
    if qtd_tarefas_em_caixas != qtd_tarefas:
        return True
    else:
        return False

def verificar_qtd_processos_na_subcaixa():
    posicao1 = pagina.find_elements(By.CLASS_NAME, "rich-tree-node-children")[cont].find_elements(By.CLASS_NAME,"rich-tree-node-text")[cont_subcaixas].text.find("(") + 1
    posicao2 = pagina.find_elements(By.CLASS_NAME, "rich-tree-node-children")[cont].find_elements(By.CLASS_NAME,"rich-tree-node-text")[cont_subcaixas].text.find(")")
    qtd_processos_na_subcaixa = int(pagina.find_elements(By.CLASS_NAME, "rich-tree-node-children")[cont].find_elements(By.CLASS_NAME,"rich-tree-node-text")[cont_subcaixas].text[posicao1:posicao2])
    return qtd_processos_na_subcaixa
# Executando o PJe Office
# "C:\Program Files (x86)\pje-office\pjeOffice.exe"

# Abrir página do Chrome
pagina = webdriver.Chrome()
# Maximizar a página
pagina.maximize_window()
# Abrir a página do PJe
pagina.get("https://pje.tjdft.jus.br/")
# Localizar o iframe no qual está o botão de login
WDW(pagina, 60).until(EC.frame_to_be_available_and_switch_to_it((By.XPATH, '//*[@id="ssoFrame"]')))
# Clicar no botão de login
pagina.find_element(By.ID, "kc-pje-office").click()
# Aguardar a inserção da senha
input("Insira a senha.")
# Abrir página de trabalho no PJe
pagina.get("https://pje.tjdft.jus.br/pje/Painel/painel_usuario/list.seam")
# criar uma lista de caixas já verificadas
nomes_caixas = []
cont = 0
while cont < 100:
    caixas = atualizar_caixas()
    if cont < len(caixas[0]):
        # Verificar se a caixa ainda não foi analisada
        posicao = caixas[0][cont].text.find("(") - 1
        if caixas[0][cont].text[:posicao] not in nomes_caixas:
            # Identificar caixas sem caixas internas
            if caixas[0][cont].text.count("caixas") == 0:
                caixas[0][cont].click()
                # Aguardar a presença dos processos
                WDW(pagina, 60).until(EC.presence_of_all_elements_located((By.CLASS_NAME, "rich-table-row")))
                # Verificar a quantidade de páginas dentro da caixa
                qtd_pgs = int(pagina.find_element(By.CLASS_NAME, "rich-inslider-right-num").text)
                # Verificar quantidade de processos na página
                qtd_processos_na_pg = len(pagina.find_elements(By.CLASS_NAME, "rich-table-row"))
                cont_processos = 0
                while cont_processos <= qtd_processos_na_pg:
                    # Pegar o número do processo pela id
                    num_processo = pagina.find_elements(By.CLASS_NAME, "rich-table-row")[cont_processos].find_elements(By.CLASS_NAME, "rich-table-cell")[1].find_element(By.TAG_NAME, "h6").get_attribute("id")
                    pagina.find_elements(By.CLASS_NAME, "fa-external-link")[cont_processos].click()
                    clicar = pagina.find_element(By.ID, num_processo).find_element(By.TAG_NAME, "a")
                    pagina.find_elements(By.CLASS_NAME, "rich-table-row")[cont_processos].find_elements(By.CLASS_NAME,"rich-table-cell")[0].find_elements(By.CLASS_NAME, "btn-default")[2].click()
                    AC(pagina).move_to_element(pagina.find_elements(By.CLASS_NAME, "rich-table-row")[cont_processos].find_elements(By.CLASS_NAME,"rich-table-cell")[0]).click(pagina.find_elements(By.CLASS_NAME, "rich-table-row")[cont_processos].find_elements(By.CLASS_NAME,"rich-table-cell")[0].find_element(By.CLASS_NAME, "fa-external-link")).perform()
                    # //*[@id="consultaProcessoGridList:0:j_id259:toolbarDiv:j_id273"]
                    # <a href="/pje/Processo/ConsultaProcesso/Detalhe/detalheProcessoVisualizacao.seam?id=1966659&amp;ca=e0e7dfe2095de738ec995bb3bbc9243821894194c3fc621cdb8500306fdf0abe705a8242644943cc0f4719ffb31071f29ccef146623d632a&amp;tab=form&amp;idTaskInstance=3441631275" id="consultaProcessoGridList:0:j_id259:toolbarDiv:j_id273" target="1966659popUpDetalhesProcessoTrf" onclick="openPopUp('1966659popUpDetalhesProcessoTrf', '');" title="Ver Detalhes" class="btn btn-default"><i class="fa fa-external-link"></i></a>

                # AQUI VERIFICA OS PROCESSOS NESSA CAIXA

            else:
                # Clicar na seta para expandir a caixa principal
                caixas[1][cont].click()
                # AQUI VERIFICA AS SUBCAIXAS E OS PROCESSOS NAS SUBCAIXAS
                # Aguardar as subcaixas aparecerem
                WDW(caixas[0], 60).until(EC.presence_of_all_elements_located((By.CLASS_NAME, "rich-tree-node-text")))
                # Encontrar a qtd de subcaixas dentro da caixa analisada
                qtd_subcaixas = pagina.find_elements(By.CLASS_NAME, "rich-tree-node-children")[cont].find_elements(By.CLASS_NAME, "rich-tree-node-text")
                cont_subcaixas = 0
                while cont_subcaixas < qtd_subcaixas:
                    # Verificar quantos processos tem na subcaixa a ser analisada
                    qtd_processos_na_subcaixa = verificar_qtd_processos_na_subcaixa()
                    cont_processos_subcaixas = 0
                    if qtd_processos_na_subcaixa > 0:
                        # Clicar na subcaixa para analisar os processos
                        menu = pagina.find_elements(By.CLASS_NAME, "rich-tree-node-children")[cont]
                        clicar = pagina.find_elements(By.CLASS_NAME, "rich-tree-node-children")[cont].find_elements(By.CLASS_NAME, "rich-tree-node-text")[cont_subcaixas]
                        AC(pagina).move_to_element(menu).click(clicar).perform()
                        while qtd_processos_na_subcaixa < cont_processos_subcaixas:




                # <td class="rich-tree-node-text rich-tree-node-selected" id="naoSegredoForm:idTarefaTree:j__id226:0:j__id226:0::j_id236:text" xmlns:rich="http://richfaces.ajax4jsf.org/rich" rich:ajaxsingle="true" rich:highlightedclass="rich-tree-node-highlighted" rich:selectedclass="rich-tree-node-selected"><span class="mr-10">(6)</span>**NAYARA - RECESSO FORENSE</td>
